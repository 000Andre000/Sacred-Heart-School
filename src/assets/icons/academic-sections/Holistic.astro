---
import { type IconProps as Props } from "../types";

const { height, width } = Astro.props as Props;
---

<svg
    version="1.1"
    id="HolisticDevelopmentIcon"
    xmlns="http://www.w3.org/2000/svg"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    viewBox="0 0 64 64"
    fill="currentColor"
    class={`h-${height} w-${width}`}
>
    <g>
        <!-- Outer circle representing overall well-being -->
        <circle cx="32" cy="32" r="30" stroke="currentColor" stroke-width="4" fill="none" />
        <!-- Sections representing different aspects of development -->
        <path d="M32,2 A30,30 0 0,1 62,32" stroke="currentColor" stroke-width="4" fill="none" />
        <path d="M32,2 A30,30 0 0,1 2,32" stroke="currentColor" stroke-width="4" fill="none" />
        <path d="M32,62 A30,30 0 0,1 2,32" stroke="currentColor" stroke-width="4" fill="none" />
        <path d="M32,62 A30,30 0 0,1 62,32" stroke="currentColor" stroke-width="4" fill="none" />
        <!-- Central dot symbolizing the core or balance point -->
        <circle cx="32" cy="32" r="6" fill="currentColor" />
    </g>
</svg>
