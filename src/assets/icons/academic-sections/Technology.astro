---
import { type IconProps as Props } from "../types";

const { height, width } = Astro.props as Props;
---

<svg
    version="1.1"
    id="TechnologyIntegrationIcon"
    xmlns="http://www.w3.org/2000/svg"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    viewBox="0 0 64 64"
    fill="currentColor"
    class={`h-${height} w-${width}`}
>
    <g>
        <!-- Globe -->
        <circle cx="32" cy="32" r="14" stroke="currentColor" stroke-width="2" fill="none" />

        <!-- Horizontal and Vertical lines for latitude and longitude -->
        <line x1="18" y1="32" x2="46" y2="32" stroke="currentColor" stroke-width="2" />
        <line x1="32" y1="18" x2="32" y2="46" stroke="currentColor" stroke-width="2" />

        <!-- Diagonal lines -->
        <line x1="23" y1="23" x2="41" y2="41" stroke="currentColor" stroke-width="2" />
        <line x1="23" y1="41" x2="41" y2="23" stroke="currentColor" stroke-width="2" />

        <!-- Interconnected nodes -->
        <circle cx="32" cy="14" r="2" fill="currentColor" />
        <circle cx="32" cy="50" r="2" fill="currentColor" />
        <circle cx="14" cy="32" r="2" fill="currentColor" />
        <circle cx="50" cy="32" r="2" fill="currentColor" />

        <!-- Connecting lines between nodes -->
        <line x1="32" y1="14" x2="32" y2="32" stroke="currentColor" stroke-width="1" />
        <line x1="32" y1="50" x2="32" y2="32" stroke="currentColor" stroke-width="1" />
        <line x1="14" y1="32" x2="32" y2="32" stroke="currentColor" stroke-width="1" />
        <line x1="50" y1="32" x2="32" y2="32" stroke="currentColor" stroke-width="1" />
    </g>
</svg>
